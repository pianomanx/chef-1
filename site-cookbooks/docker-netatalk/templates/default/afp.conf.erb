[Global]
hostname = <%= @hostname %>
mimic model = RackMac

; enable guest and user based access
uam list = uams_guest.so uams_dhx2.so uams_dhx.so
guest account = nobody

; send the log file to console
log file = /dev/stdout
log level = default:info

[Backups]
path = /data/Backups
vol size limit = 512000
time machine = yes

[Media]
path = /data/Media

[Downloads]
path = /data/Downloads

[Documents]
path = /data/Documents

