# Global parameters
[global]
  workgroup = WORKGROUP
  netbios name = SERVER
  server string = <%= @hostname %>
  map to guest = nobody
  log file = /var/log/samba/log.%m
  max log size = 50
  socket options = TCP_NODELAY SO_RCVBUF=8192 SO_SNDBUF=8192
  security = Share

[Data]
  comment = Data Directory
  path = /data
  read only = no
  guest ok = yes
